      var adName = "Sold_Result_Page_List";
      var adContainer = document.getElementById(adName);

      var modifier = '__' +
        Hemnet.page.layoutProfile.charAt(0).toUpperCase() +
        Hemnet.page.layoutProfile.slice(1);

      // cu = Content Unit (in Cxense)
      var cuKey = adName + modifier;

      var ad = document.createElement('div');
      ad.id = cuKey;

      adContainer.appendChild(ad);
      var cuUrl = "//eas4.emediate.eu/eas?cre=mu;js=y;pageviewid=;target=_blank&cu_key=" + cuKey + ";c1-1-0=1;c1-7-0=1";
      EAS_load_fif(cuKey, '/EAS_fif.html', cuUrl, 0, 0);
    })(document);
  </script>
