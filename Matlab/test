  <script async>
    (function(document) {
      var adName = "Sold_Result_Page_Panorama";
      var adContainer = document.getElementById(adName);

      var modifier = '__' +
        Hemnet.page.layoutProfile.charAt(0).toUpperCase() +
        Hemnet.page.layoutProfile.slice(1);

      // cu = Content Unit (in Cxense)
      var cuKey = adName + modifier;

      var ad = document.createElement('div');
      ad.id = cuKey;

      adContainer.appendChild(ad);
      var cuUrl = "//eas4.emediate.eu/eas?cre=mu;js=y;pageviewid=;target=_blank&cu_key=" + cuKey + ";c1-1-0=1;c1-7-0=1";
      EAS_load_fif(cuKey, '/EAS_fif.html', cuUrl, 0, 0);
    })(document);
  </script>
      <script src="http://assets.hemnet.se/assets/javascripts/hemnet-maps.min.js?1464942104" type="text/javascript"></script>
      <script type="text/javascript">
        $(function() {
          var loadGoogleMaps = function () {
            var script = document.createElement('script');
            script.src = "https://maps.googleapis.com/maps/api/js?libraries=geometry,places\u0026amp;v=3.22\u0026amp;client=gme-hemnetservicehns\u0026amp;region=SE\u0026amp;language=sv/&callback=initInlineMap";
            document.body.appendChild(script);
          }

          window.initInlineMap = function () {
            var projectAds = {}; 
            var status = "sold";
            var searchKey = "a50784abc93389a14b7c79b923ef6f5de62275f6";
            var locations = [];
            var markerAppearanceFactory = new Hemnet.Maps.MarkerAppearanceFactory(
                status, "http://assets.hemnet.se/assets");
            var tileserverHosts = ["http://a.tiles.hemnet.se/","http://b.tiles.hemnet.se/","http://c.tiles.hemnet.se/","http://d.tiles.hemnet.se/"];
            var map = $(".property-map");

            new Hemnet.Components.Maps.InlineSearchMap(
                status, searchKey, locations, map, markerAppearanceFactory, tileserverHosts, projectAds);
          };

          // Fallback for browsers not supporting `innerWidth`
          var windowWidth = window.innerWidth || document.body.clientWidth;
          if (windowWidth >= 569) {
            loadGoogleMaps();
          }
        });
      </script>

